    <fields>
        <fullName>Aging__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(Number_of_Days_Overdue__c=0),&quot;0&quot;,
		IF(AND(Number_of_Days_Overdue__c&gt;0,Number_of_Days_Overdue__c&lt;=30), &quot;1-30&quot;, 
		IF(AND(Number_of_Days_Overdue__c&gt;30,Number_of_Days_Overdue__c&lt;=60),&quot;31-60&quot;,
		IF(AND(Number_of_Days_Overdue__c&gt;60,Number_of_Days_Overdue__c&lt;=90),&quot;61-90&quot;,
		IF(AND(Number_of_Days_Overdue__c&gt;90,Number_of_Days_Overdue__c&lt;=120),&quot;91-120&quot;,
		IF(AND(Number_of_Days_Overdue__c&gt;120,Number_of_Days_Overdue__c&lt;=180),&quot;121-180&quot;,
		IF(Number_of_Days_Overdue__c&gt;180,&quot;180+&quot;,&quot;&quot;)))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>It is used to determine whether a company carries overdue debtors that may never repay their bills.</inlineHelpText>
        <label>Aging</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
