    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>ACH_Fields_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  NOT($Setup.ACH_Parameters__c.Use_Lock_Based_ACH__c),
ACH_On__c ,
OR(ISBLANK( ACH_Routing_Number__c ),ISBLANK( ACH_Account_Number__c ), ISBLANK( ACH_Debit_Amount__c ), ISBLANK( ACH_Next_Debit_Date__c ))
)</errorConditionFormula>
        <errorMessage>One of the required ACH fields is missing:
Routing number, Account number, Debit amount or Next debit date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Accrual_start_days_negative_check</fullName>
        <active>true</active>
        <errorConditionFormula>Accrual_Start_Days__c &lt; 0</errorConditionFormula>
        <errorMessage>Accrual start days cannot be negative.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amz_Last_Accrual_Date_Check</fullName>
        <active>true</active>
        <description>For Amz Based loans, the last accrual date cannot be moved back.</description>
        <errorConditionFormula>AND (ISCHANGED(Last_Accrual_Date__c),PRIORVALUE(Last_Accrual_Date__c) =  DATE(3000,12,31),ISPICKVAL(Product_Type__c, &apos;Amz Based Loan&apos;),
CONTAINS( TEXT(Loan_Status__c), &apos;Active&apos;))</errorConditionFormula>
        <errorMessage>For Amz Based loans, the last accrual date cannot be moved back.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Application_Date_Check</fullName>
        <active>false</active>
        <errorConditionFormula>Application_Date__c &gt; TODAY()</errorConditionFormula>
        <errorMessage>Application Date cannot be greater than calendar date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CLLoanCRM_Borrower_Check</fullName>
        <active>true</active>
        <description>Either Group or Client should be specified.</description>
        <errorConditionFormula>AND(NOT($Setup.Org_Parameters__c.Integrate_Salesforce_CRM__c), OR(ISNULL(Client__c),ISBLANK(Client__c )), OR(ISNULL(Group__c),ISBLANK(Group__c)))</errorConditionFormula>
        <errorMessage>Either Group or Client should be provided.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Capitalization_Enabled</fullName>
        <active>true</active>
        <description>Capitalization is supported only if Interest Posting is Enabled on a contract.</description>
        <errorConditionFormula>AND( NOT( Is_Interest_Posting_Enabled__c),  Is_Capitalization_Enabled__c )</errorConditionFormula>
        <errorDisplayField>Is_Capitalization_Enabled__c</errorDisplayField>
        <errorMessage>Capitalization is not supported, Kindly enable Interest Posting first.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Center_Loan_Validation</fullName>
        <active>false</active>
        <description>This validation rule is deprecated.</description>
        <errorConditionFormula>AND(
ISPICKVAL( Loan_Product_Name__r.Loan_Product_Type__c  , &apos;Center-Group-Client&apos;),
 OR(
         ISBLANK( Client__r.Center__c ) ,
         ISBLANK( Client__r.Group__c )
  )
)</errorConditionFormula>
        <errorMessage>The Loan Product is of type &apos;Center-Group-Client (Center Loan)&apos;.  Client must be associated with a Center and a Group.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Group_Loan_Product</fullName>
        <active>false</active>
        <errorConditionFormula>OR(
AND(
ISPICKVAL( Loan_Product_Name__r.Loan_Product_Type__c , &apos;Group&apos;),
NOT($Setup.Org_Parameters__c.Integrate_Salesforce_CRM__c),
ISBLANK( Group__c)
),
AND(
ISPICKVAL( Loan_Product_Name__r.Loan_Product_Type__c , &apos;Group&apos;),
$Setup.Org_Parameters__c.Integrate_Salesforce_CRM__c,
ISBLANK( Account__c )
)
)</errorConditionFormula>
        <errorMessage>This is an Group loan. You must specify an account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Individual_Loan_Product</fullName>
        <active>false</active>
        <errorConditionFormula>OR(
AND(
ISPICKVAL( Loan_Product_Name__r.Loan_Product_Type__c , &apos;Client&apos;),
NOT($Setup.Org_Parameters__c.Integrate_Salesforce_CRM__c),
ISBLANK( Client__c)
),
AND(
ISPICKVAL( Loan_Product_Name__r.Loan_Product_Type__c , &apos;Client&apos;),
$Setup.Org_Parameters__c.Integrate_Salesforce_CRM__c,
ISBLANK(  Contact__c )
)
)</errorConditionFormula>
        <errorMessage>This is an individual loan. You must specify a contact.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Product_Type</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Product_Type__c,&quot;&quot;)</errorConditionFormula>
        <errorMessage>Product Type Cannot be Null</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Top_Up_Loan</fullName>
        <active>true</active>
        <errorConditionFormula>Loan_Product_Name__r.Is_Sub_Product__c == true &amp;&amp;   ISPICKVAL(Loan_Product_Name__r.Parent_Loan_Product__r.Status__c, &apos;Inactive&apos;)</errorConditionFormula>
        <errorMessage>Top up loan&apos;s parent must be active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Client_Coaplicant_validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($Setup.Org_Parameters__c.Integrate_Salesforce_CRM__c)
,
OR(
        ( NOT(ISBLANK(Co_Applicant__c)) &amp;&amp; NOT(ISBLANK(Client__c )) &amp;&amp; Co_Applicant__c =  Client__c ),
        ( NOT(ISBLANK(Guarantor__c )) &amp;&amp; NOT(ISBLANK(Co_Applicant__c )) &amp;&amp; Co_Applicant__c =  Guarantor__c )
       )
)</errorConditionFormula>
        <errorDisplayField>Co_Applicant__c</errorDisplayField>
        <errorMessage>Co-applicant can not be same as Client or Guarantor</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Client_Guarantor</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($Setup.Org_Parameters__c.Integrate_Salesforce_CRM__c)
,OR(
